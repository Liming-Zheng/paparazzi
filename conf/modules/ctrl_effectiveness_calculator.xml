<!DOCTYPE module SYSTEM "module.dtd">

<module name="ctrl_effectiveness_calculator" dir="ctrl">
  <doc>
    <description>
      Function which calculates the control effectiveness matrix on the fly
    </description>
  </doc>
  <settings>
  	<dl_settings>
  		<dl_settings NAME="inertia">
  			<dl_setting var="I.xx" min="0.017897" step="0.001" max="0.06617232" shortname="I_xx" param="I_XX"/>
  			<dl_setting var="I.yy" min="0.003390" step="0.001" max="0.04914925" shortname="I_yy" param="I_YY"/>
  			<dl_setting var="I.zz" min="0.01752587" step="0.001" max="0.020337" shortname="I_zz" param="I_ZZ"/>
  		</dl_settings>
  	</dl_settings>
  </settings>
  <header>
    <file name="ctrl_effectiveness_calculator.h"/>
  </header>
  <periodic fun="ctrl_eff_periodic()" freq="512"/>
  <makefile>
    <file name="ctrl_effectiveness_calculator.c"/>
  </makefile>
</module>

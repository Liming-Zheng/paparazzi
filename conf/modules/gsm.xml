<!DOCTYPE module SYSTEM "module.dtd">

<module name="gsm">
  <header>
    <file name="gsm.h"/>
  </header>
  <periodic fun="gsm_init()" period="2." autorun="TRUE"/>
  <periodic fun="gsm_send_report()" period="60." autorun="FALSE"/>
  <event fun="gsm_event()"/>
  <makefile>
    <file name="gsm.c"/>
  </makefile>
</module>


<!DOCTYPE module SYSTEM "module.dtd">

<module name="gcnet_rpm_range">
  <doc>
    <description>
    This module is used to test the gcnet controller which can dynamically change the maximum RPMs.
    </description>
  </doc>

  <header>
    <file name="nn_parameters.h"/>
    <file name="nn_operations.h"/>
    <file name="gcnet_main.h"/>
  </header>
  <init fun="gcnet_init()"/>
  <periodic fun="gcnet_run()" freq="512" />
  <!--datalink message="EXTERNAL_VISION" fun="external_vision_update(buf)"/-->

<!-- Code to be able to change Max RPM in Paparazzi Center, not sure if correct...
  <settings >
    < dl_settings name =" Max_RPM ">
      <dl_setting var =" max_rpm_input_nn " min ="11000" step ="50" max="12000 " shortname =" max_rpm_nn "/>
    </ dl_settings >
  </ settings >
-->

  <makefile>
    <file name="nn_parameters.c"/>
    <file name="nn_operations.c"/>
    <file name="gcnet_main.c"/>
  </makefile>
</module>

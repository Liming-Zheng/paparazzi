<flight_plan SECURITY_HEIGHT="25" alt="450" ground_alt="420" lat0="44.000000" lon0="1" max_dist_from_home="500" name="Generic" qfu="270">
  <dl_settings>
    <dl_setting MAX="0.2" MIN="-0.2" STEP="0.01" VAR="ir_roll_neutral"/>
    <dl_setting MAX="0.2" MIN="-0.2" STEP="0.01" VAR="ir_pitch_neutral"/>
    <dl_setting MAX="15000" MIN="5000" STEP="100" VAR="roll_pgain"/>
    <dl_setting MAX="20000" MIN="10000" STEP="100" VAR="pitch_pgain"/>
    <dl_setting MAX="-0.1" MIN="-1" STEP="0.05" VAR="course_pgain"/>
    <dl_setting MAX="2" MIN="0" STEP="1" VAR="pprz_mode"/>
  </dl_settings>
  <waypoints>
    <waypoint NAME="HOME" X="0" Y="0"/>
    <waypoint NAME="1" X="200" Y="0"/>
    <waypoint NAME="2" X="0" Y="200"/>
    <waypoint alt="450." name="S1" x="-181.37499716" y="122.374998203"/>
    <waypoint alt="450." name="S2" x="136.75000196" y="308.874998301"/>
  </waypoints>
  <blocks>
    <block NAME="wait GPS">
      <while COND="!GPS_FIX_VALID(gps_mode)"/>
    </block>
    <block name="init">
      <while COND="10>block_time"/>
      <set value="gps_utm_east/100" var="nav_utm_east0"/>
      <set value="gps_utm_north/100" var="nav_utm_north0"/>
      <set value="gps_utm_zone" var="nav_utm_zone0"/>
      <set value="gps_alt/100+50" var="waypoints[WP_HOME].a"/>
      <set value="TRUE" var="moved_waypoints[WP_HOME]"/>
      <set value="gps_alt/100+50" var="waypoints[WP_1].a"/>
      <set value="TRUE" var="moved_waypoints[WP_1]"/>
      <set value="gps_alt/100+50" var="waypoints[WP_2].a"/>
      <set value="TRUE" var="moved_waypoints[WP_2]"/>
      <deroute BLOCK="circlehome"/>
    </block>
    <block NAME="circlehome">
      <circle RADIUS="75" WP="HOME"/>
    </block>
    <block NAME="stack 1">
      <circle RADIUS="75" WP="1"/>
    </block>
    <block NAME="stack 2">
      <circle RADIUS="75" WP="2"/>
    </block>
    <block NAME="survey">
      <survey_rectangle grid="70" wp1="S1" wp2="S2"/>
    </block>
  </blocks>
</flight_plan>

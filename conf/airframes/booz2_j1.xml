   <define name="HOVER_KP"    value="-200"/>
   <define name="HOVER_KD"    value="-100"/>
   <define name="HOVER_KI"    value="-50"/>
  </section>


 <section name="GUIDANCE_H" prefix="BOOZ2_GUIDANCE_H_">
   <define name="PGAIN" value="-100"/>
   <define name="DGAIN" value="-100"/>
   <define name="IGAIN" value="-0"/>
 </section>

 <section name="JOYSTICK" prefix="BOOZ2_JOYSTICK_">
   <define name="SP_PHI_COEF" value="2860"/> <!-- MAX SP = 20 deg ( RadOfDeg(20 / 128) << 20 ) -->
   <define name="SP_THETA_COEF" value="2860"/> <!-- MAX SP = 20 deg ( RadOfDeg(20 / 128) << 20 ) -->
 </section>

 <section name="BAT">
   <define name="MILLIAMP_PER_PERCENT" value="0.86"/>
   <define name="CATASTROPHIC_BAT_LEVEL" value="9.3" unit="V"/>
 </section>



  <makefile>

include $(PAPARAZZI_SRC)/conf/autopilot/booz2_autopilot.makefile
include $(PAPARAZZI_SRC)/conf/autopilot/booz2_test_progs.makefile
include $(PAPARAZZI_SRC)/conf/autopilot/booz2_simulator.makefile

  </makefile>

</airframe>

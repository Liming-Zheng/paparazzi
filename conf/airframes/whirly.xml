    <define name="DEFAULT_WHIRLY_RPM_PGAIN_SLOPE" value="0."/>
    <define name="DEFAULT_WHIRLY_RPM_PGAIN_INTERCEPT" value="15000"/>
  </section>

  <makefile>

#WHIRLY_USER = poine
#WHIRLY_HOST = whirly-dev
#WHIRLY_DIR  = /home/poine


  <servos>
    <servo name="MOTOR_1" no="0" min="1000" neutral="1050" max="2000"/>
    <servo name="MOTOR_2" no="1" min="1000" neutral="1050" max="2000"/>
    <servo name="MOTOR_3" no="2" min="1000" neutral="1050" max="2000"/>
    <servo name="MOTOR_4" no="3" min="1000" neutral="1050" max="2000"/>

    <servo name="EL_1" no="8" min="650" neutral="1500" max="2350"/>
    <servo name="EL_2" no="9" min="650" neutral="1500" max="2350"/>
    <servo name="EL_3" no="10" min="650" neutral="1500" max="2350"/>
    <servo name="EL_4" no="11" min="650" neutral="1500" max="2350"/>

    <servo name="AZ_1" no="4" min="1050" neutral="1500" max="1950"/>
    <servo name="AZ_2" no="5" min="1050" neutral="1500" max="1950"/>
    <servo name="AZ_3" no="6" min="1050" neutral="1500" max="1950"/>
    <servo name="AZ_4" no="7" min="1050" neutral="1500" max="1950"/>

  </servos>

  <commands>
    <axis name="ROLL" failsafe_value="0"/>
    <axis name="PITCH" failsafe_value="0"/>
    <axis name="YAW" failsafe_value="0"/>
    <axis name="THRUST" failsafe_value="0"/>
    <axis name="RADIO_TH_HOLD" failsafe_value="-9600"/>
  </commands>

  <rc_commands>
    <set command="THRUST" value="@THROTTLE"/>
    <set command="ROLL" value="@YAW"/>
    <set command="PITCH" value="@PITCH/2"/>
    <set command="YAW" value="-@ROLL/4"/>
  </rc_commands>


  <command_laws>
    <call fun="overactuated_mixing_run()"/>
    <let var="th_hold" value="LessThan(RadioControlValues(RADIO_TH_HOLD), -4800)"/>
    <set servo="MOTOR_1" value="($th_hold? -9600 : overactuated_mixing.commands[0])"/>
    <set servo="MOTOR_2" value="($th_hold? -9600 : overactuated_mixing.commands[1])"/>
    <set servo="MOTOR_3" value="($th_hold? -9600 : overactuated_mixing.commands[2])"/>
    <set servo="MOTOR_4" value="($th_hold? -9600 : overactuated_mixing.commands[3])"/>
    <set servo="EL_1" value="(overactuated_mixing.commands[4])"/>
    <set servo="EL_2" value="(overactuated_mixing.commands[5])"/>
    <set servo="EL_3" value="(overactuated_mixing.commands[6])"/>
    <set servo="EL_4" value="(overactuated_mixing.commands[7])"/>
    <set servo="AZ_1" value="(overactuated_mixing.commands[8])"/>
    <set servo="AZ_2" value="(overactuated_mixing.commands[9])"/>
    <set servo="AZ_3" value="(-overactuated_mixing.commands[10])"/>
    <set servo="AZ_4" value="(-overactuated_mixing.commands[11])"/>
  </command_laws>

